<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e_monsite.nazim_app.binazim">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="Digital"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Digital"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity2"
            android:label="BinaryTodecimal"
            android:parentActivityName=".Beta"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity3"
            android:label="AlltoDecimal"
            android:parentActivityName=".Beta"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity4"
            android:label="DecimalToAll"
            android:parentActivityName=".Beta"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e_monsite.nazim_app.binazim.MainActivity" />
        </activity>
        <activity
            android:name=".Activity5"
            android:label="DecimalTOHexa"
            android:parentActivityName=".Beta"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e_monsite.nazim_app.binazim.MainActivity" />
        </activity>
        <activity
            android:name=".Binary"
            android:label="ToBinary"
            android:parentActivityName=".Beta"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e_monsite.nazim_app.binazim.MainActivity" />
        </activity>
        <activity
            android:name=".Beta"
            android:label="Menu"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e_monsite.nazim_app.binazim.MainActivity" />
        </activity>
        <activity android:name=".Arithmetic"></activity>
    </application>

</manifest>
